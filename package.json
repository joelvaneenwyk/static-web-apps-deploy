{
  "name": "static-web-apps-deploy",
  "version": "1.0.0",
  "description": "This Github Action enables developers to build and publish their applications to Azure App Service Static Web Apps. This action utilizes [Oryx](https://github.com/microsoft/Oryx) to detect and build an application, then uploads the resulting application content, as well as any Azure Functions, to Azure.",
  "packageManager": "npm@10.8.0+sha256.00f8f31f9847fec0e5615b5571555c3e766a4601431c0817bf6e4b73668571ae",
  "engines": {
    "node": ">=18",
    "npm": "^10.8"
  },
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "cd test && hugo --config hugo.toml --gc --minify",
    "start": "cd test && hugo server",
    "deploy": "netlify deploy --build --prod --open --site static-web-apps-deploy",
    "test": "netlify deploy",
    "prepack": "npm run build",
    "format": "prettier --write ."
  },
  "files": [
    "entrypoint.sh",
    "compose.yml",
    "Dockerfile",
    "action.yml",
    "LICENSE",
    "README.md",
    "SECURITY.md",
    "package.json",
    "package-lock.json"
  ],
  "author": "Joel Van Eenwyk",
  "license": "MIT",
  "dependencies": {
    "autoprefixer": "^10.4.19",
    "cross-env": "^7.0.3",
    "hugo-extended": "^0.126.1",
    "netlify-cli": "^17.23.5",
    "postcss": "^8.4.38",
    "postcss-cli": "^11.0.0",
    "prettier": "^3.2.5",
    "sass": "^1.77.2",
    "shx": "^0.3.4"
  }
}
